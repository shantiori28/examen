@{
}

@model List<Category>
<h1>
    Lista de categoria
</h1>
<table class="table table-bordered">

    <tr>
        <th>
            Nombre de categoria
        </th>
        <th>
            Precio
        </th>

    </tr>
    <tbody>
        @foreach (var obj in Model.OrderBy(u => u.Price))
        {
            <tr>
                <td>
                    @obj.Name
                </td>
                <td>
                    @obj.Price
                </td>
            </tr>
        }
    </tbody>
</table>